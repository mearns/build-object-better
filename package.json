{
  "name": "build-object-better",
  "version": "1.0.2",
  "description": "Utilities for building out an object from properties",
  "main": "index.js",
  "keywords": [
    "object builder",
    "objects",
    "properties"
  ],
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:mearns/build-object-better.git"
  },
  "author": "Brian Mearns <mearns.b@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mearns/build-object-better/issues"
  },
  "homepage": "https://github.com/mearns/build-object-better/",
  "engines": {
    "node": ">=8 <=12"
  },
  "scripts": {
    "clean": "rimraf dist/ out/ build/ coverage/ .nyc_output/",
    "pretty": "pretty-quick --staged",
    "pretty-all": "prettier --write src/**/*.js ./*.js ./*.json ./*.yaml ./*.yml",
    "test": "nyc npm run test-only",
    "test-only": "mocha test/",
    "verify": "npm run check && npm run test",
    "check:readme": "remark --no-stdout README.md",
    "check:js": "eslint ./ test/ --ext .js --format codeframe",
    "check": "npm run check:js && npm run check:readme",
    "compile": "echo \"Nothing to build for this project, runs from src.\"",
    "docs": "jsdoc --recurse --destination out/jsdocs --package package.json --configure jsdoc.conf.json index.js",
    "build": "npm-run-all clean verify compile"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.1.0",
    "jsdoc": "3.6.3",
    "jsdoc-to-markdown": "5.0.1",
    "mocha": "5.2.0",
    "node": "13.3.0",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "remark-cli": "7.0.0",
    "rimraf": "2.6.2",
    "sinon": "7.2.3",
    "sinon-chai": "3.3.0",
    "webpack": "4.28.1"
  },
  "dependencies": {}
}
